<?xml version="1.0" encoding="UTF-8"?>
<bundle>
   <UIAdminToolbarPortlet>
      <action>
         <AddPage>Добавить страницу</AddPage>
         <EditNavigation>Навигация</EditNavigation>
         <Content>Содержимое</Content>
         <Editor>Edit</Editor>
         <Page>Page</Page>
         <Site>Сайт</Site>
         <Layout>Edit Layout</Layout>
         <siteLayout>Layout</siteLayout>
         <ManagePages>Manage Pages</ManagePages>
         <GroupSite>Group Site</GroupSite>
         <Properties>#{UISEOToolbarForm.label.seo}</Properties>
         <AddSite>Add Site</AddSite>
      </action>
<ManageSites>Manage Sites</ManageSites>
<ManageGroupSites>Manage Group Sites</ManageGroupSites>
   </UIAdminToolbarPortlet>

  <UISEOForm>
    <label>
      <title>Название:</title>  
      <description>Описание:</description>
      <keywords>Ключевые слова:</keywords>
      <language>Язык:</language>
      <robots>Роботы:</robots>
      <sitemap>Карта сайта:</sitemap>
      <sitemapvisible>Visible in sitemap</sitemapvisible>
  <isInherited>Inherited from parent page</isInherited>
      <priority>Приоритет:</priority>
      <frequency>Частота:</frequency>
      <select-language>Select Language</select-language>
      <option>
        <Always>Всегда</Always>
        <Hourly>Каждый час</Hourly>
        <Daily>Ежедневно</Daily>
        <Weekly>Еженедельно</Weekly>
        <Monthly>Ежемесячно</Monthly>
        <Yearly>Ежегодно</Yearly>
        <Never>Никогда</Never>
      </option> 
    </label>
 
    <action>
      <Save>#{word.save}</Save>
      <Cancel>#{word.cancel}</Cancel>
    </action>
    <msg>
      <repository-exception>An unexpected error occurred. Cannot save page metadata.</repository-exception>
      <confirm-delete>Вы уверены, что вы хотите удалить эти данные SEO?</confirm-delete>
    </msg>
    <title>
    <seo-delete>Delete</seo-delete>
    </title>
<help> 
  <priority>Диапазон допустимых значений от 0.0 до 1.0.</priority>
  <description>Help searchers: let them know that the following page has a link with what they are looking for.</description>
  <keywords>Most important terms in the page must be separated by a comma.</keywords>  
    </help>  
  </UISEOForm>
  <UISEOPopupWindow>
  <title>
      <UISEOForm>#{UISEOToolbarForm.title.seo}</UISEOForm>
  </title>
  </UISEOPopupWindow>    
  <UISEOToolbarForm>
  <title>
      <seo>Управление SEO</seo>
  </title>
  <label>
    <seo>SEO</seo>
  </label>
  </UISEOToolbarForm>  
  <FloatNumberValidator>
    <msg>
      <Invalid-number>Priority field has an invalid value. Please enter a number between 0.0 and 1.0.</Invalid-number>
    </msg>
  </FloatNumberValidator>
  
  <Locale>
    <label>      
      <it>Italian</it>      
      <fr>Французский</fr>
      <en>Английский</en>      
    </label>
  </Locale>
  
</bundle>
