<?xml version="1.0" encoding="UTF-8"?>
<bundle>
   <UIAdminToolbarPortlet>
      <action>
         <AddPage>Adicionar página</AddPage>
         <EditNavigation>Navegação</EditNavigation>
         <Content>Content</Content>
         <Editor>Edit</Editor>
         <Page>Page</Page>
         <Site>Site</Site>
         <Layout>Editar Layout</Layout>
         <siteLayout>Layout</siteLayout>
         <ManagePages>Gerenciar páginas</ManagePages>
         <GroupSite>Site do grupo</GroupSite>
         <Properties>SEO</Properties>
         <AddSite>Adicionar Site</AddSite>
      </action>
<ManageSites>Gerenciar sites</ManageSites>
<ManageGroupSites>Gerenciar sites do grupo</ManageGroupSites>
   </UIAdminToolbarPortlet>

  <UISEOForm>
    <label>
      <title>Título:</title>  
      <description>Description:</description>
      <keywords>Keywords:</keywords>
      <language>Language:</language>
      <robots>Robôs:</robots>
      <sitemap>Sitemap:</sitemap>
      <sitemapvisible>Visível no mapa do site</sitemapvisible>
  <isInherited>Inherited from parent page</isInherited>
      <priority>Priority:</priority>
      <frequency>Frequency:</frequency>
      <select-language>Selecionar idioma</select-language>
      <option>
        <Always>Always</Always>
        <Hourly>De hora em hora</Hourly>
        <Daily>Daily</Daily>
        <Weekly>Weekly</Weekly>
        <Monthly>Mensalmente</Monthly>
        <Yearly>Anualmente</Yearly>
        <Never>Never</Never>
      </option> 
    </label>
 
    <action>
      <Save>Save</Save>
      <Cancel>Cancel</Cancel>
    </action>
    <msg>
      <repository-exception>Ocorreu um erro inesperado. Não é possível salvar os metadados da página.</repository-exception>
      <confirm-delete>Tem certeza que deseja excluir esses dados SEO?</confirm-delete>
    </msg>
    <title>
    <seo-delete>Excluir</seo-delete>
    </title>
<help> 
  <priority>Valid values range from 0.0 to 1.0.</priority>
  <description>Ajudar buscadores: deixe-os saber que a página à seguir tem um link com o que eles estão procurando.</description>
  <keywords>Os termos mais importantes na página devem ser separados por vírgula.</keywords>  
    </help>  
  </UISEOForm>
  <UISEOPopupWindow>
  <title>
      <UISEOForm>Gerenciamento de SEO</UISEOForm>
  </title>
  </UISEOPopupWindow>    
  <UISEOToolbarForm>
  <title>
      <seo>Gerenciamento de SEO</seo>
  </title>
  <label>
    <seo>SEO</seo>
  </label>
  </UISEOToolbarForm>  
  <FloatNumberValidator>
    <msg>
      <Invalid-number>Priority field has an invalid value. Please enter a number between 0.0 and 1.0.</Invalid-number>
    </msg>
  </FloatNumberValidator>
  
  <Locale>
    <label>      
      <it>Italiano</it>      
      <fr>French</fr>
      <en>English</en>      
    </label>
  </Locale>
  
</bundle>
