<?xml version="1.0" encoding="UTF-8"?>
<bundle>
   <UIUserPlatformToolBarCreatePortlet>
      <label>
         <topic>Topic</topic>
         <poll>Poll</poll>
         <eventtask>Evento/tarea</eventtask>
         <upload>Cargar un archivo</upload>
         <wiki>Página wiki</wiki>

      </label>
   </UIUserPlatformToolBarCreatePortlet>
   <UICreateList>
      <label>
         <topic>Tema</topic>
         <poll>Encuesta</poll>
         <CreateANew>Crear un nuevo {1}:</CreateANew>
         <NoForum>Lo sentimos, no hay foro disponible aún para crear un {1}. Empezar por crear su propio espacio.</NoForum>
         <RequireSelectForum>Por favor seleccione un foro.</RequireSelectForum>
      </label>
      <action>
         <Next>Next</Next>
         <Cancel>Cancel</Cancel>
      </action>
   </UICreateList>
   <UICreateTopic>
      <label>
         <SelectAForum>Seleccione un foro</SelectAForum>
         <location>En lugar</location>
         <forumId>Y el foro</forumId>
         <FilterForums>Filtrar Foros</FilterForums>
      </label>
   </UICreateTopic>
   <UICreatePoll>
      <label>
         <SelectAForum>Select Forum</SelectAForum>
         <location>In Location</location>
         <forumId>And Forum</forumId>
         <FilterForums>Filter Forums</FilterForums>
      </label>
   </UICreatePoll>
   <UICreateWiki>
      <label>
         <title>Crear una nueva página Wiki:</title>
         <location>In Location</location>
      </label>
   </UICreateWiki>
   <UICreateEvent>
      <label>
         <Title>Title</Title>
         <Choix>Agregar nuevo:</Choix>
         <EndEvent>To</EndEvent>
         <StartEvent>From</StartEvent>
         <Calendar>Select Calendar</Calendar>
         <all-day>All Day</all-day>
         <defaultCalendarId>Default</defaultCalendarId>
      </label>
      <msg>
         <summary-field-required>Se requiere el campo "Título".</summary-field-required>
         <fromDate-format>El formato de fecha "Desde" es incorrecto.</fromDate-format>
         <toDate-format>El formato de fecha "Hasta" es incorrecto.</toDate-format>
         <logic-required>Lo sentimos, no puede crear un evento que termina antes de que empiece.</logic-required>
         <cant-add-event-on-remote-calendar>No puede agregar evento al calendario remoto</cant-add-event-on-remote-calendar>
         <add-successfully>
            <Event>El evento ha sido añadido a {1}</Event>
            <Task>La tarea ha sido añadida a {1}</Task>
         </add-successfully>
         <add-unsuccessfully>{0} no fue agregado.</add-unsuccessfully>
      </msg>
      <action>
         <Next>#{word.save}</Next>
         <Cancel>Cancel</Cancel>
      </action>
   </UICreateEvent>
    <UIUploadFile>
        <Select>Select File</Select>
    </UIUploadFile>
   <Choix>
      <label>
         <Event>Event</Event>
         <Task>Task</Task>
      </label>
   </Choix>
</bundle>
