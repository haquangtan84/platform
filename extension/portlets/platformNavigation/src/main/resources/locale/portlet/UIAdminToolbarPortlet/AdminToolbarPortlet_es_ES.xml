        <Always>Always</Always>
        <Hourly>Hourly</Hourly>
        <Daily>Daily</Daily>
        <Weekly>Weekly</Weekly>
        <Monthly>Monthly</Monthly>
        <Yearly>Yearly</Yearly>
        <Never>Never</Never>
      </option> 
    </label>
 
    <action>
      <Save>Save</Save>
      <Cancel>Cancel</Cancel>
    </action>
    <msg>
      <repository-exception>Se produjo un error inesperado. No puede guardar los metadatos de la página.</repository-exception>
